<body>
    
<h1>punto diesyseis</h1>



    <script>
    
    var nums=new Array;
    var contar;
    var control=0;
    for(var x=0;x<=10;x++){ 
        nums[x]=(prompt("Ingresar"));
        contar=nums[x].countTokens();
        if(contar>control){ 
            control=contar;
        } 
    } 
    document.write("<p>"+control+"<p>");
    
    
    
    </script>
</body>
